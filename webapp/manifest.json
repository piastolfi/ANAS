{
	"_version": "1.7.0",
	"sap.app": {
		"id": "Z_GEST_FERIEZ_GEST_FERIE",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "Z_GEST_FERIEZ_GEST_FERIE.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.collaboration": {},
				"sap.m": {},
				"sap.ui.comp": {},
				"sap.ui.core": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "Z_GEST_FERIEZ_GEST_FERIE.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "Z_GEST_FERIEZ_GEST_FERIE.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"transition": "slide"
			},
			"routes": [
				{
					"pattern": "",
					"name": "home",
					"target": "home"
				},
				{
					"name": "creation",
					"pattern": "creationPage",
					"greedy": false,
					"target": [
						"creation"
					]
				},
				{
					"name": "creationWithParams",
					"pattern": "creationPage/from-{dateFrom}/to-{dateTo}",
					"greedy": false,
					"target": [
						"creation"
					]
				}
			],
			"targets": {
				"home": {
					"viewName": "Home",
					"viewLevel": 1
				},
				"creation": {
					"viewName": "Creation",
					"viewId": "creation",
					"viewLevel": 1
				},
				"creationWithParams": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "Creation",
					"viewId": "creation",
					"viewLevel": 1
				}
			}
		}
	}
}